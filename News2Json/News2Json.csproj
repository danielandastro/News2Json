<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp2.2;net462;net47</TargetFrameworks>
    <PackOnBuild>true</PackOnBuild>
    <PackageId>news2json</PackageId>
    <PackageVersion>0.1</PackageVersion>
    <Authors>Daniel Shroff</Authors>
    <Description>A library to convert RSS feeds to JSON</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.1" />
    <PackageReference Include="System.ServiceModel.Syndication" Version="4.5.0" />
    <PackageReference Include="System.Xml.ReaderWriter" Version="4.3.1" />
    <PackageReference Include="NuGet.Build.Packaging" Version="0.2.0" />
  </ItemGroup>
</Project>
